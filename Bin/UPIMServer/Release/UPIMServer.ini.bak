[SERVICE]
; SERVID: 				服务器ID
; SERVICE_ADDR:            		服务器侦听的地址
; SERVICE_PORT:            		侦听端口
SERVID=1
SERVICE_ADDR=0.0.0.0
SERVICE_PORT=5555

[PUBLIC]
;conntimeout,数据库连接超时(秒)
;commtimeout,数据库执行超时(秒)
;synbegintime,同步员工信息开始时间(一天同步一次)
;synendtime,同步员工信息结束时间
;notebegintime,消息扫描开始时间(HHMMSS)
;noteendtime,消息扫描结束时间(HHMMSS)
;noteinterval,扫描间隔(秒)
;pushmsg,是否推送消息
;seqid,组时间戳
;pseqid,个人消息时间戳
;offfile,是否允许发送离线文件,离线文件需要配置网络共享文件夹
;offfilepath,离线文件存放路径
;offfilesize,离线文件最大大小，-1默认（30M），最大不超过50M
;offmsgday，离线消息有效天数
;tsrouting,TS路由，默认0，不连接。1：连接

conntimeout=5
commtimeout=5
synbegintime=160000
synendtime=170000

notebegintime=80000
noteendtime=190000
noteinterval=30
pushprdtmsg=1
pushmsg=0
seqid=0
pseqid=45
offfile=1
offfilepath=D:\test1
offfilesize=-1
offmsgday=7
tsrouting=1

[clientctrl]
ctrl_on=1
TradeQueryAsk=1
clientroomfile=1
forbidfile=0
forbidofffile=0
forbidroomoption=0
roomfile=1

[dbconfig]
;数据库个数
num=1
;默认数据库
sel=0
DataSource_1=192.168.7.158
DataBase_1=TQ3
UserName_1=sa
Password_1=sa
